{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "tsmlxtras/meetings-block",
  "title": "Meeting Blocks",
  "category": "embed",
  "icon": "groups",
  "attributes": {
    "blockType": {
      "type": "string",
      "default": "tsml_ui"
    },
    "showCount": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "align": true,
    "anchor": true,
    "className": true,
    "color": {
      "link": true,
      "background": true,
      "text": true
    },
    "typography": {
      "fontSize": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalTextTransform": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontAppearance": true,
        "textTransform": true
      }
    }
  },
  "textdomain": "tsmlxtras-meetings-block",
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:../../../assets/css/tsmlxtras.css"
}
